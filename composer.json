{
    "name": "boenrobot/big-xlsx-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for generating large multi-sheeted xlsx files with low memory usage. Fork of bassim/BigXlsxBundle, for compatibility with newer PHP versions.",
    "keywords": ["php", "xlsx",  "excel", "spreadsheet", "csv"],
    "homepage": "http://github.com/boenrobot/BigXlsxBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas Simons",
            "email": "bassim@xs4all.nl"
        },
        {
            "name": "Vasil Rangelov",
            "email": "boen.robot@gmail.com"
        }
    ],
    "require": {
        "phpoffice/phpspreadsheet": "^1.12",
        "ext-zip": "*"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "symfony/symfony": "2.3.*",
        "phpunit/phpunit": "^5"
    },
    "conflict": {
        "bassim/big-xlsx-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Bassim\\BigXlsxBundle\\Tests\\": "tests",
            "Bassim\\BigXlsxBundle\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
